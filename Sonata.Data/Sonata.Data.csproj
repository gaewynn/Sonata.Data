    <Authors>gaewynn</Authors>
    <Company>Sonata</Company>
    <Description>Sonata.Data</Description>
    <Copyright>Copyright @ 2018 Sonata</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Gaewynn/Sonata.Data</PackageProjectUrl>
